<div class="container">
	<br>
	<br>
	<fieldset >
	<legend>Datos Estudiante</legend>
	
		<div class="row">
			<div class="col-md-10">
			<br>
			<br>
				<div class="row">
					<div class="col-md-4">
					<label style=" width: 150px;">Nº Carnet:</label>
					<input type="text" name="NumCarnet" class="form-control">
					</div>
					<div class="col-md-4">
					<label style=" width: 150px;">Primer Nombre:</label>
					<input type="text" name="Primernombre" class="form-control">
					</div>
					<div class="col-md-4">
					<label style=" width: 150px;">Segundo Nombre:</label>
					<input type="text" name="Segundornombre" class="form-control">
					</div>
				</div>
				<div class="row">
					<div class="col-md-4">
					<label style=" width: 150px;">Primer Apellido:</label>
					<input type="text" name="Primerapellido" class="form-control">
					</div>
					<div class="col-md-4">
					<label style=" width: 150px;">Segundo Apellido:</label>
					<input type="text" name="Segundoapellido" class="form-control">
					</div>
					<div class="col-md-4">
					<label style=" width: 150px;">Edad:</label>
					<input type="text" name="Edad" class="form-control">
					</div>
				</div>
			</div>
			<div class="col-md-2">
				<%= image_tag "adefault_user.png", :alt => "User",:class=>"PictureDefault", :title => "User"%>
			</div>
		</div>
		<div class="row">
			 <div class="col-md-12">
			 <label style=" width: 150px;">Direccion:</label>
			 <textarea type="text" name="Direccionestu" class="form-control"></textarea>
			 </div>
		</div>
		<div class="row">
			 <div class="col-md-3">
				 <label style=" width: 150px;">Tipo Sanguineo:</label>
				 <input type="text" name="tiposanguineo" class="form-control"></input>
			 </div>
			 <div class="col-md-3">
				 <label style=" width: 150px;">Estado Civil:</label>
				 <input type="text" name="Estadocivil" class="form-control"></input>
			 </div>
			 <div class="col-md-3">
				 <label style=" width: 150px;">Celular:</label>
				 <input type="text" name="Celular" class="form-control"></input>
			 </div>
			 <div class="col-md-3">
				 <label style=" width: 150px;">Telefono:</label>
				 <input type="text" name="Telefono" class="form-control"></input>
			 </div>
		</div>
		<div class="row">
			 <div class="col-md-3">
				 <label style=" width: 150px;">Genero:</label>
				 <input type="text" name="Genero" class="form-control"></input>
			 </div>
			 <div class="col-md-3">
			  <label style=" width: 150px;">Municipio:</label>
			  <select name="" id="" class="form-control">
				  <option value="0">Seleccione</option>
			  </select>
			 </div>
			 <div class="col-md-3">
			 </div>
			  <div class="col-md-3">
			 </div>
		</div>
	</fieldset>
	<br>
	<fieldset >
	<legend>Estudios</legend>
		<div class="row">
			<div class="col-md-3">
				<label style=" width: 150px;">Estudio:</label>
				<input type="text" name="Estudio" class="form-control"></input>
			</div>
			<div class="col-md-3">
				<label style=" width: 150px;">Año:</label>
				<input type="text" name="Ano" class="form-control"></input>
			</div>
			<div class="col-md-3">
				<label style=" width: 150px;">Presedencia:</label>
				<input type="text" name="Presedencia" class="form-control"></input>
			</div>
			<div class="col-md-3">
